{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpdev/example-dynamic-block",
  "version": "0.1.0",
  "title": "Example Dynamic Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "carouselInfiniteLoop": {
      "type": "boolean",
      "default": false
    },
    "carouselAutoplay": {
      "type": "boolean",
      "default": true
    },
    "displayFeaturedImage": {
      "type": "boolean",
      "default": true
    },
    "dynamicBullets": {
      "type": "boolean",
      "default": false
    },
    "order": {
      "type": "string",
      "default": "desc"
    }
  },
  "textdomain": "example-dynamic-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}